{
    "name": "@hckrnews/logger",
    "description": "Winston logger for Hckrnews",
    "version": "0.3.2",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://hckr.news/"
    },
    "license": "MIT",
    "scripts": {
        "lint": "eslint src/*.js --config .eslintrc",
        "lint:report": "eslint src/*.js --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "test": "c8 node --test src/",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --omit=dev"
    },
    "type": "module",
    "files": [
        "src/logger.js",
        "src/models/logger.js",
        "src/models/schemas/logger.js",
        "src/models/enums/level.js",
        "src/models/mutators/logger.js",
        "src/loggers/index.js",
        "src/loggers/console.js",
        "src/loggers/sentry.js",
        "src/loggers/combined-file.js",
        "src/loggers/error-file.js",
        "src/helpers/stackdriver.js"
    ],
    "main": "src/logger.js",
    "devDependencies": {
        "@hckrnews/error": "^2.0.0",
        "@hckrnews/eslint-config": "^3.0.0",
        "c8": "^9.0.0",
        "eslint": "^8.23.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^48.0.6",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.0.1",
        "eslint-plugin-sonarjs": "^0.23.0",
        "jscpd": "^3.2.1",
        "sinon": "^17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hckrnews/logger"
    },
    "engines": {
        "node": ">= 18.13"
    },
    "keywords": [
        "javascript",
        "sentry",
        "logging",
        "error"
    ],
    "dependencies": {
        "@hckrnews/enum": "^3.0.0",
        "@hckrnews/mutator": "^0.3.2",
        "@hckrnews/objects": "^7.0.0",
        "@sentry/integrations": "^7.36.0",
        "@sentry/node": "^7.36.0",
        "@sentry/tracing": "^7.36.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "winston": "^3.8.2",
        "winston-transport": "^4.5.0",
        "winston-transport-sentry-node": "^2.7.0"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    }
}